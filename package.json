{
  "name": "cloud-picture-centring",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build --dest=dist_vue",
    "lint": "vue-cli-service lint",
    "build-background": "webpack-cli --mode=production --config='background-main.webpack-config.js'",
    "build-background-dev": "webpack-cli --mode=development --config='background-main.webpack-config.js'",
    "build-background-release": "webpack-cli --mode=development --config='background-main.webpack-config.js'",
    "build-dev": "vue-cli-service build --mode development --dest=dist_vue",
    "build-release": "vue-cli-service build --mode release --dest=dist_vue",
    "ebuild": "vue-cli-service electron:build",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "eserve-dev": "vue-cli-service electron:serve --mode development",
    "eserve-release": "vue-cli-service electron:serve --mode release",
    "new": "plop",
    "pack-vue": "asar pack dist_vue dist_electron/dist_vue.asar",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "stylelint": "stylelint 'src/**/*.vue' --config '.stylelintrc' --fix",
    "cz": "yarn log && git add . && git cz",
    "log": "conventional-changelog --config ./node_modules/vue-cli-plugin-commitlint/lib/log -i CHANGELOG.md -s -r 0"
  },
  "main": "background.js",
  "dependencies": {
    "animejs": "^3.1.0",
    "asar": "^2.0.1",
    "clipboard": "^2.0.4",
    "driver.js": "^0.9.8",
    "echarts": "^4.5.0",
    "element-ui": "^2.4.5",
    "eventemitter3": "^4.0.0",
    "express": "^4.17.1",
    "file-type": "^12.4.0",
    "idb-js": "^1.3.1",
    "js-base64": "^2.5.1",
    "js-md5": "^0.7.3",
    "js-sha1": "^0.6.0",
    "jszip-utils": "^0.1.0",
    "live2d-widget": "^3.1.4",
    "lottie-web": "^5.5.9",
    "mainto-jssdk": "^1.8.2475-beta.release.2.5.3.4",
    "md5.js": "^1.3.5",
    "mime": "^2.4.4",
    "moment": "^2.24.0",
    "mousetrap": "^1.6.3",
    "node-rsa": "^1.0.6",
    "nprogress": "^0.2.0",
    "request": "^2.88.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-webpack-plugin": "^1.0.3",
    "tui-editor": "^1.4.10",
    "upyun": "^3.3.11",
    "uuid": "^3.3.3",
    "v-charts": "^1.19.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "webpack-cli": "^3.3.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "commitizen": "^4.0.3",
    "commitlint": "^8.2.0",
    "conventional-changelog-cli": "^2.0.28",
    "electron": "^6.0.0",
    "electron-icon-builder": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^3.0.9",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "vue-cli-plugin-axios": "^0.0.4",
    "vue-cli-plugin-commitlint": "^1.0.7",
    "vue-cli-plugin-electron-builder": "^1.4.0",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": "./.eslintrc.js",
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/vue-cli-plugin-commitlint/lib/cz"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
